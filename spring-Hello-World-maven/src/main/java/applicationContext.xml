<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="student" class="com.zensar.spring.beans.Student">
	
		<property name="rollNumber" value="12"></property>
		<property name="studentName" value="Ram"></property>
		<property name="studentMarks" value="233"></property>
		<property name="address">
				<bean id="addr" class="com.zensar.spring.beans.Address">
				<property name="pinCode" value="411014"></property>
				<property name="city" value="Pune"></property>
				<property name="state" value="Maha"></property>
			</bean>
		</property>
		
	</bean>
	
